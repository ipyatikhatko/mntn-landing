<body class="overflow-hidden">
  {{> preloader}}
  <div id="root" class="hide-content">
    {{> nav}}
    {{> hero}}
    <section id="steps" class="steps-container">
      {{#each steps}}
        {{> step this}}
      {{/each}}
    </section>
  </div>
  <script src="main.js"></script>
</body>
</html>